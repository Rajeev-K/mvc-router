{
    "compilerOptions": {
        "noEmitOnError": true,
        "removeComments": false,
        "skipLibCheck": true,
        "sourceMap": true,
        "allowJs": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "noImplicitThis": true,
        "lib": [ "es5", "dom", "dom.iterable", "es2015.promise" ],
        "target": "es5",
        "module": "esnext",
        "jsx": "react"
    },
    "exclude": [
        "node_modules",
        "wwwroot"
    ]
}
